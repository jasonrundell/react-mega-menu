{
  "name": "@jasonrundell/react-mega-menu",
  "version": "1.3.1",
  "author": "jasonrundell <jason.rundell@gmail.com>",
  "source": "./src/index.js",
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": "./dist/index.modern.js",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "build": "npm run build:micro",
    "build:parcel": "parcel build index.js --no-minify --no-cache --public-url /",
    "build:micro": "microbundle --jsx React.createElement",
    "dev": "microbundle watch",
    "format": "prettier --write src/**/*.{js,jsx}"
  },
  "dependencies": {
    "husky": "^7.0.1",
    "node-sass": "^7.0.0",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "microbundle": "^0.13.3",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.3.2",
    "prop-types": "^15.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jasonrundell/react-mega-menu.git"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  },
  "license": "MIT"
}
